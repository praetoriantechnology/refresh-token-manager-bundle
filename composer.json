{
  "name": "praetoriantechnology/refresh-token-manager-bundle",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {"Praetorian\\RefreshTokenManagerBundle\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {
      "Praetorian\\Tests\\RefreshTokenManagerBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix": "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix",
    "test": "./vendor/phpunit/phpunit/phpunit"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpunit/phpunit": "^9.0"
  },
  "require": {
    "symfony/config": "^5.2 || ^4.4",
    "symfony/dependency-injection": "^5.2 || ^4.4",
    "symfony/http-kernel": "^5.2 || ^4.4",
    "praetoriantechnology/cache-service": "^1.3.0",
    "symfony/mercure-bundle": "^0.3.2",
    "monolog/monolog": "^2.2",
    "praetoriantechnology/venus-orm-pack": "dev-master",
    "gesdinet/jwt-refresh-token-bundle": "^0.12.0"
  }
}
